        "drupal/mail_safety": "1.x-dev",
        "grasmash/drupal-security-warning": "1.1.1",
        "jakoch/phantomjs-installer": "2.1.1-p09",
        "jcalderonzumba/mink-phantomjs-driver": "0.3.3",
        "mikey179/vfsStream": "1.6.5",
        "phing/phing": "2.16.1",
        "phpunit/phpunit": "4.8.36",
        "se/selenium-server-standalone": "3.10.0",
        "squizlabs/php_codesniffer": "2.9.1"
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
          "drupal/node_title_validation": {
            "Unable to import configuration": "https://www.drupal.org/files/issues/2855289-7.patch",
            "Unique node title validation restricts node edit": "https://www.drupal.org/files/issues/unique_node_title-2828307-5.patch"
          },
          "drupal/swiftmailer": {
            "Unit Testing is failing on testMassageMessageBody()": "https://www.drupal.org/files/issues/2018-03-13/swiftmailer-unittest.patch"
          },
          "drupal/typogrify": {
            "Invalid placeholder (version) in string": "https://www.drupal.org/files/issues/2018-03-16/2953490-invalid-placeholder-fix.patch"
          },
          "drupal/empty_fields": {
            "Plugin form not shown": "https://www.drupal.org/files/issues/2927913-8.patch"
          },
          "drupal/core": {
            "Cannot use relationship for rendered entity on Views": "https://www.drupal.org/files/issues/2457999-164.patch",
            "Views RSS feeds have validation issues": "https://www.drupal.org/files/issues/d8-rss-validation-atom-link-missing-1337894-25.patch",
            "Allow users with access to unpublished nodes to create unpublished books": "https://www.drupal.org/files/issues/26552-133.patch",
            "Cannot save unpublished versions of published content for users without manage book privileges": "https://www.drupal.org/files/issues/2918537-36.patch",
            "Views filter 'Published status or admin user' not checking 'View any unpublished content' permission": "https://www.drupal.org/files/issues/2019-03-12/3030477-5.patch",
            "Allow menu items which link to unpublished nodes to be selected in the parent item selector": "https://www.drupal.org/files/issues/drupal-core-allow-menu-items-to-link-to-unpublished-2807629-7-8.3.x.patch"
          },
          "drupal/ie8": {
            "drupal.js Incompatible in D8.5": "https://www.drupal.org/files/issues/2018-03-30/drupal_js.patch"
          },
          "drupal/context_metadata": {
            "Check if default metadata is not null": "https://www.drupal.org/files/issues/2018-11-26/3003858-4-Check-if-default-metadata-is-not-null.patch"
          },
          "drupal/masquerade_float_block": {
            "Masquerade Float Block appears for anonymous users":"https://www.drupal.org/files/issues/masquerade_float_block-2917207-3.patch"
          },
          "drupal/rules": {
            "WSOD 'Cannot set a list with a non-array value'": "https://www.drupal.org/files/issues/2019-04-12/rules-2987505-19-Cannot-set-list-to-nonarray.patch",
            "Condition UserHasRole throws an error":"https://www.drupal.org/files/issues/2816157-10.patch"
          }
        }
    },
    "autoload": {
        "psr-4": {
            "govCMS\\Core\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "conflict": {
        "drupal/core": "7.*",
        "drush/drush": "<8.1.10",
        "twig/twig":"^1.40.0"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer package"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "package": "govCMS\\Core\\Composer\\Package::execute",
        "nuke": "rm -r -f bin docroot vendor"
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
