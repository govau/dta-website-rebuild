	"extra": {
		"drupal-scaffold": {
			"initial": {
				"sites/default/default.services.yml": "sites/default/services.yml",
				"sites/default/default.settings.php": "sites/default/settings.php"
			},
			"excludes": [
				"sites/development.services.yml"
			]
		},
		"installer-types": ["bower-asset", "npm-asset"],
		"installer-paths": {
			"docroot/core": ["type:drupal-core"],
			"docroot/modules/contrib/{$name}": ["type:drupal-module"],
			"docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
			"docroot/profiles/{$name}": ["type:drupal-profile"],
			"docroot/themes/contrib/{$name}": ["type:drupal-theme"],
			"docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
			"docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
			"drush/contrib/{$name}": ["type:drupal-drush"]
		},
		"patches": {
			"drupal/rabbit_hole": {
			  "Error on empty redirect path": "https://www.drupal.org/files/issues/2020-04-27/2926929Z-29.patch"
			},
			"drupal/search_api_page": {
			  "Search block form should define a base form ID": "https://www.drupal.org/files/issues/2019-07-02/3063684-2--search_form_base_id.patch"
			},
			"drupal/swiftmailer": {
				"Unit Testing is failing on testMassageMessageBody()": "https://www.drupal.org/files/issues/2018-03-13/swiftmailer-unittest.patch"
			},
			"drupal/core": {
				"Cannot use relationship for rendered entity on Views": "https://www.drupal.org/files/issues/2020-05-30/cannot-use-relationship-2457999-236.patch",
				"Views RSS feeds have validation issues": "https://www.drupal.org/files/issues/d8-rss-validation-atom-link-missing-1337894-25.patch",
				"Allow users with access to unpublished nodes to create unpublished books": "https://www.drupal.org/files/issues/2019-04-03/26552-144.patch",
				"Cannot save unpublished versions of published content for users without manage book privileges": "https://www.drupal.org/files/issues/2020-07-14/2918537-61.patch",
				"Allow menu items which link to unpublished nodes to be selected in the parent item selector": "https://www.drupal.org/files/issues/drupal-core-allow-menu-items-to-link-to-unpublished-2807629-7-8.3.x.patch"
			},
			"drupal/ie8": {
				"drupal.js Incompatible in D8.5": "https://www.drupal.org/files/issues/2019-08-20/2957350-2-drupal-js-incompatible-in-D8-5.patch"
			},
			"drupal/context_metadata": {
				"Check if default metadata is not null": "https://www.drupal.org/files/issues/2018-11-26/3003858-4-Check-if-default-metadata-is-not-null.patch"
			},
			"drupal/rules": {
				"Condition UserHasRole throws an error": "https://www.drupal.org/files/issues/2816157-10.patch"
			},
			"drupal/entity_usage": {
				"HTMLLink Plugin only works with PublicStreamWrapper": "https://www.drupal.org/files/issues/2019-09-19/entity_usage-htmllink-streamwrapper-2973297-7.patch"
			},
			"drupal/video_embed_field": {
				"Youtube Privacy Enhanced Mode (using -nocookie.com) (D8)": "https://www.drupal.org/files/issues/2019-06-08/video_embed_field_youtube_nocookie_2973246_41.patch"
			},
			"drupal/google_analytics": {
				"Allow taxonomy term name in custom dimension settings": "https://www.drupal.org/files/issues/2019-04-24/allow_taxonomy_term_name_custom_dimension_settings_305080_2.patch"
			},
			"drupal/dynamic_entity_reference": {
				"Errors when new entity types are added (in certain cases)":"https://www.drupal.org/files/issues/2020-05-31/3099176-2.x-9.patch"
			},
			"drupal/captcha": {
				"Captcha Session ID broken with cacheable captcha backends":"https://www.drupal.org/files/issues/2020-07-30/3089263-48.patch"
			}
		},
		"merge-plugin": {
			"include": [
				"docroot/modules/contrib/webform/composer.libraries.json"
			]
		},
		"symfony": {
			"require": "3.4.*"
		}
	},
	"autoload": {
		"psr-4": {
			"govCMS\\Core\\": "src"
		}
	},
	"config": {
		"bin-dir": "bin/",
		"sort-packages": true,
		"optimize-autoloader": true,
		"secure-http": false,
		"platform": {
				"php": "7.1.20"
		}
	},
	"conflict": {
		"drupal/core": "7.*",
		"drush/drush": "<8.1.10",
		"twig/twig": "^1.40.0"
	},
	"scripts": {
		"post-install-cmd": [
			"PhantomInstaller\\Installer::installPhantomJS",
			"@composer drupal-scaffold",
			"rm -r -f docroot/robots.txt -v"
		],
		"post-update-cmd": [
			"PhantomInstaller\\Installer::installPhantomJS",
			"@composer package"
		],
		"drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
		"package": "govCMS\\Core\\Composer\\Package::execute",
		"nuke": "rm -r -f bin docroot vendor"
	},
	"suggest": {
		"hirak/prestissimo": "^0.3",
		"rubenrua/symfony-clean-tags-composer-plugin":"^1.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
